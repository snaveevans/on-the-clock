<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="co.tylerevans.On_the_Clock"
          android:versionCode="1"
          android:versionName="1.0">
    <uses-sdk android:minSdkVersion="16"/>
    <application android:label="@string/app_name" android:icon="@drawable/ic_launcher">
        <activity android:name=".HoursToLeave"
                  android:label="@string/app_name"
                android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".CardPuncher" android:label="@string/app_name" android:screenOrientation="portrait"/>
        <activity android:name=".HoursWorked" android:label="@string/app_name" android:screenOrientation="portrait"/>
    </application>
</manifest>
